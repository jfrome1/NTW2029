{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "allowJs": true,
	  "jsx": "preserve",
	  "esModuleInterop": true,
	  "sourceMap": true,
	  "baseUrl": ".",
	  "paths": {
		"@/*": ["src/*"]
	  },
	},
	"exclude": [
	  "node_modules",
	  "public"
	]
}
